  function getPrebidAuctionLog() {
    var auctionLog, events, auctionEnd, i, j, k;
    auctionLog = {};
    events = gooad.hblog.pbjsReady ? pbjs.getEvents() : [];
    auctionEnd = false;

    // extract "auctionEnd" event and create auctionLog object
    for (i = 0; i < events.length; i++) {
      var e = events[i];
      if (e.eventType !== "auctionEnd") {
        continue;
      }

      auctionEnd = true;
      var args = e.args;
      auctionLog = {
        "auctionId": args.auctionId,
        "auctionStartTimeStamp": args.timestamp,
        "auctionEndTimeStamp": args.auctionEnd,
        "auctionEndElapsedTime": e.elapsedTime,
        "timeout": args.timeout,
        "bidLog": {}
      };

      // create bidLog from bidderRequests
      for (j = 0; j < args.bidderRequests.length; j++) {
        var bidder = args.bidderRequests[j];
        var bidderCode = bidder.bidderCode;
        for (k = 0; k < bidder.bids.length; k++) {
          var b = bidder.bids[k];
          if (!auctionLog["bidLog"][b.adUnitCode]) {
            auctionLog["bidLog"][b.adUnitCode] = {};
          }
          auctionLog["bidLog"][b.adUnitCode][b.bidId] = {
            auctionId: b.auctionId,
            adUnitCode: b.adUnitCode,
            bidderCode: bidderCode,
            mediaTypes: Object.keys(b.mediaTypes)[0],
            sizes: (function(){
              var sizes = [];
              for (var l = 0; l < b.sizes.length; l++) {
                sizes.push(b.sizes[l].join("x"));
              }
              return sizes;
            })(),
            adId: "",
            cpm: 0,
            requestTimeStamp: undefined,
            timeToRespond: undefined,
            status: ""
          };
        }
      }

      // add bidResponse to bidLog
      for (j = 0; j < args.bidsReceived.length; j++) {
        var response = args.bidsReceived[j];
        if (!auctionLog["bidLog"][response.adUnitCode] || !auctionLog["bidLog"][response.adUnitCode][response.requestId]) {
          window.gooad.Utility.message(
            "No bid request. adUnitCode:"+response.adUnitCode+" requestId:"+response.requestId);
          continue;
        }
        auctionLog["bidLog"][response.adUnitCode][response.requestId]["adId"] = response.adId;
        auctionLog["bidLog"][response.adUnitCode][response.requestId]["cpm"] = response.cpm;
        auctionLog["bidLog"][response.adUnitCode][response.requestId]["requestTimeStamp"] = response.requestTimestamp;
        auctionLog["bidLog"][response.adUnitCode][response.requestId]["timeToRespond"] = response.timeToRespond;
        auctionLog["bidLog"][response.adUnitCode][response.requestId]["status"] = response.statusMessage;
      }

      // add noBid to bidLog
      for (j = 0; j < args.noBids.length; j++) {
        var nb = args.noBids[j];
        if (!auctionLog["bidLog"][nb.adUnitCode] || !auctionLog["bidLog"][nb.adUnitCode][nb.bidId]) {
          window.gooad.Utility.message(
            "No bid request. adUnitCode:"+nb.adUnitCode+" bidId:"+nb.bidId);
          continue;
        }
        auctionLog["bidLog"][nb.adUnitCode][nb.bidId]["status"] = "noBid";
      }
      break;
    }
    if (!auctionEnd) {
      window.gooad.Utility.message('auction is not end yet.');
    }

    return auctionLog;
  }

  function getPrebidImpLog () {
    var winners = gooad.hblog.pbjsReady ? pbjs.getAllWinningBids() : [];
    var impLog =[];
    var i;
    for (i = 0; i < winners.length; i++) {
      var w = winners[i];
      impLog.push({
        auction_id: w.auctionId,
        request_id: w.requestId,
        adunit_code: w.adUnitCode
      });
    }
    return impLog;
  }
  
  function _outputTable(outputArray, exceptionMessage) {
    if (outputArray.length) {
      if (console.table) {
        console.table(outputArray);
      } else {
        for (var i = 0; i < outputArray.length; i++) {
          console.log(outputArray[i]);
        }
      }
    } else {
    console.log(exceptionMessage);
    }  
  }

  function sendLogData() {
    // initialize
    var hbLog, prebidAuctionLog, prebidImpLog, debugOutput, i, key, k;
    hbLog = {
      logging_ver: "1.0.1",
      page_setting_id: window.gooad.pageSettingID,
      nguserid: _getCookie('NGUserID'),
      device: deviceType,
      map_dfp_load: window.gooad.hblog.map_dfp_load || undefined,
      map_aps_load: window.gooad.hblog.map_aps_load || undefined,
      map_prebid_load: window.gooad.hblog.map_prebid_load || undefined,
      dfp_adslot_defined: window.gooad.hblog.dfp_adslot_defined || undefined,
      aps_bid_request: window.gooad.hblog.aps_bid_request || undefined,
      aps_bid_response: window.gooad.hblog.aps_bid_response || undefined,
      prebid_add_adunits: window.gooad.hblog.prebid_add_adunits || undefined,
      prebid_bid_request: window.gooad.hblog.prebid_bid_request || undefined,
      prebid_bid_response: window.gooad.hblog.prebid_bid_response || undefined,
      dfp_refresh: window.gooad.hblog.dfp_refresh || undefined,
      force_refresh: window.gooad.hblog.force_refresh || false,
      pbjs_ready: window.gooad.hblog.pbjsReady || false,
      pbjs_ready_time: window.gooad.hblog.pbjsReadyTime || undefined,
      apstag_ready: window.gooad.hblog.apstagReady || false,
      apstag_ready_time: window.gooad.hblog.apstagReadyTime || undefined,
      prebid_auction_id: undefined,
      prebid_log: {}
    };
    prebidAuctionLog = {};
    prebidImpLog = [];
    debugOutput = [];

    // send hbLog to TD
    prebidAuctionLog = getPrebidAuctionLog();
    hbLog.prebid_auction_id = prebidAuctionLog.auctionId;
    hbLog.prebid_log = JSON.stringify(prebidAuctionLog);
    window.gooad.Utility.message("prebidAuctionLog");
    window.gooad.Utility.message(prebidAuctionLog);
    window.gooad.Utility.message("hbLog");
    window.gooad.Utility.message(hbLog);
    if(loggingConfig.sendDataOn){
      _sendToTD(loggingConfig.tdTables.hbLogTable, hbLog);
    }
    // send impLog to TD
    setTimeout(function () {
        prebidImpLog = getPrebidImpLog();
        window.gooad.Utility.message("prebidImpLog");
        window.gooad.Utility.message(prebidImpLog);
        for (i = 0; i < prebidImpLog.length; i++) {
          if(loggingConfig.sendDataOn){
            _sendToTD(loggingConfig.tdTables.prebidImpTable, prebidImpLog[i]);
          }
        }
        // debug output for prebidImpLog
        if(window.top.location.href.indexOf('gooad_debug')> -1){
          _outputTable(prebidImpLog, 'NO prebid Winner');
        }
      }, loggingConfig.timeToGetImpLog);
    // debug output for prebidAuctionLog
    if(window.top.location.href.indexOf('gooad_debug')> -1){
      if (prebidAuctionLog.bidLog) {
        for (key in prebidAuctionLog.bidLog) {
          if (prebidAuctionLog.bidLog.hasOwnProperty(key)) {
            for (k in prebidAuctionLog["bidLog"][key]) {
              if (prebidAuctionLog["bidLog"][key].hasOwnProperty(k)) {
                prebidAuctionLog["bidLog"][key][k]["sizeString"] = prebidAuctionLog["bidLog"][key][k]["sizes"].join(",");
                debugOutput.push(prebidAuctionLog["bidLog"][key][k]);
              }
            }
          }
        }
      }
      _outputTable(debugOutput, 'NO prebid responses');
    }
  }

}());