            sixSense_company_state: attributesData.sixSense_company_state,
            sixSense_country: attributesData.sixSense_country,
            sixSense_domain: attributesData.sixSense_domain,
            sixSense_employee_range: attributesData.sixSense_employee_range,
            sixSense_industry: attributesData.sixSense_industry,
            sixSense_revenue_range: attributesData.sixSense_revenue_range,
            sixSense_segments: attributesData.sixSense_segments && attributesData.sixSense_segments.join("|"),
