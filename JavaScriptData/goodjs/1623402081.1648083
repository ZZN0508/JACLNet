function setMemberPlanInfo() {
    var cookies = ajq.cookie("hsMemberPlanInfo");
    var planInfo = cookies ? cookies.split("|") : false;
    if (planInfo && planInfo.length === 4) {
        var data = {};

        if (planInfo[0] !== 0) data.planId = planInfo[0];
        data.maxPlanId = planInfo[1];
        data.isInOrganisation = planInfo[2];
        data.trialStatus = planInfo[3];

        for (var prop in data) {
            if (isNaN(parseInt(data[prop]))) {
                delete data[prop];
            }
        }

        Evergage.setCustomFields(data, Evergage.Scope.Request);
    }
}

function onFormSubmit(form, action, config, callback) {
    if (typeof config === "function" && callback === undefined) {
        callback = config;
    } else if (typeof config !== "object") {
        config = {};
    }

    Evergage.util.runWhenReady(
        function () {
            return ajq("form").is(form);
        },
        function () {
            Evergage.safeSubmit(form, function () {
                var data = {
                    userEmail: ajq.trim(ajq(this).find("#Email").val()),
                    industry: ajq.trim(ajq(this).find("#Industry option:checked").val()),
                    companyName: ajq.trim(ajq(this).find("#Company").val()),
                    numberOfEmployees: ajq.trim(ajq(this).find("#No_of_Employees__c option:checked").val()),
                    companyRevenue: ajq.trim(ajq(this).find("#Annual_Company_Revenue__c option:checked").val()),
                }
                for (var prop in data) {
                    if (!data[prop]) {
                        delete data[prop];
                    }
                }
                if (typeof callback === "function") {
                    data = callback.call(form, data);
                }
                Evergage.setCustomFields(data, Evergage.Scope.Request);
                !!config.sendActionEventOnNextPage ?
                    Evergage.sendActionEventOnNextPage(action, null, 5000 || config.sendActionEventOnNextPageDuration) :
                    Evergage.trackAction(action);
            });
        },
        1000,
        10
    );
}

function findInArray(array, callback, reverse) {
    if (!Array.isArray(array) || typeof callback !== "function")
        return null;
    var i, element;
    if (reverse) {
        for (i = array.length - 1; i >= 0 ; i--) {
            element = array[i];
            if (callback(element, i, array)) {
                return element;
            }
        }
    } else {
        for (i = 0; i < array.length; i++) {
            element = array[i];
            if (callback(element, i, array)) {
                return element;
            }
        }
    }
    return null;
}

function watchArrayForNewItems(array, callback) {
    if (!Array.isArray(array) || typeof callback !== "function")
        return null;
    if (callback(array.slice())) {
        return;
    }
    var oldLength = array.length;
    Evergage.util.runWhenReady(function () {
        var newLength = array.length;
        if (newLength > oldLength) {
            var newItems = array.slice(oldLength, newLength);
            var result = callback(newItems);
            oldLength = newLength;
            return result;
        }
    }, new Function());
}

watchArrayForNewItems(window.dataLayer, function (items) {
    var attributesData = findInArray(items, function (item) {
        return item.sixSense_city || item.sixSense_company_name || item.sixSense_company_state || item.sixSense_country || item.sixSense_domain || item.sixSense_employee_range || item.sixSense_industry || item.sixSense_revenue_range || item.sixSense_segments
